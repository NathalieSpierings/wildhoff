@mixin keyframesAnimate($name: empty, $duration: empty, $function: empty){
	animation-name: $name;
	animation-duration: $duration;
	animation-timing-function: $function;
	animation-fill-mode: backwards;
	animation-iteration-count: none;
}
.slick-active {
	.leftright-1 {
		@include keyframesAnimate(fadeInLeft, .5s, ease-in);
	}
	.leftright-2 {
		@include keyframesAnimate(fadeInLeft, .4s, ease-in);
	}
	.leftright-3 {
		@include keyframesAnimate(fadeInLeft, .3s, ease-in);
	}
	.rightleft-1 {
		@include keyframesAnimate(fadeInRight, .5s, ease-in);
	}
	.rightleft-2 {
		@include keyframesAnimate(fadeInRight, .4s, ease-in);
	}
	.rightleft-3 {
		@include keyframesAnimate(fadeInRight, .3s, ease-in);
	}
	.topbottom-1 {
		@include keyframesAnimate(fadeInDown, .5s, ease-in);
	}
	.topbottom-2 {
		@include keyframesAnimate(fadeInDown, .4s, ease-in);
	}
	.topbottom-3 {
		@include keyframesAnimate(fadeInDown, .3s, ease-in);
	}
	.bottomtop-1 {
		@include keyframesAnimate(fadeInUp, .5s, ease-in);
	}
	.bottomtop-2 {
		@include keyframesAnimate(fadeInUp, .4s, ease-in);
	}
	.bottomtop-3 {
		@include keyframesAnimate(fadeInUp, .3s, ease-in);
	}
}

