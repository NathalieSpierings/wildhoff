// MIXIN /////////////////////////////
@mixin rtl-margin-left($value){
	{% if settings.enable_rtl %} 
		margin-right: $value;
	{% else %}
		margin-left: $value;
	{% endif %}
}
@mixin rtl-margin-right($value){
	{% if settings.enable_rtl %} 
		margin-left: $value;
	{% else %}
		margin-right: $value;
	{% endif %}
}
@mixin rtl-margin($t, $r, $b, $l){
	{% if settings.enable_rtl %} 
		margin: $t $l $b $r;
	{% else %}
		margin: $t $r $b $l;
	{% endif %}
}
@mixin rtl-padding-left($value){
	{% if settings.enable_rtl %} 
		padding-right: $value;
	{% else %}
		padding-left: $value;
	{% endif %}
}
@mixin rtl-padding-right($value){
	{% if settings.enable_rtl %} 
		padding-left: $value;
	{% else %}
		padding-right: $value;
	{% endif %}
}
@mixin rtl-padding($t, $r, $b, $l){
	{% if settings.enable_rtl %} 
		padding: $t $l $b $r;
	{% else %}
		padding: $t $r $b $l;
	{% endif %}
}
@mixin rtl-border-left($value){
	{% if settings.enable_rtl %} 
		border-right: $value;
	{% else %}
		border-left: $value;
	{% endif %}
}
@mixin rtl-border-right($value){
	{% if settings.enable_rtl %} 
		border-left: $value;
	{% else %}
		border-right: $value;
	{% endif %}
}
@mixin rtl-left($value){
	{% if settings.enable_rtl %} 
		right: $value;
	{% else %}
		left: $value;
	{% endif %}
}
@mixin rtl-right($value){
	{% if settings.enable_rtl %} 
		left: $value;
	{% else %}
		right: $value;
	{% endif %}
}
@mixin rtl-float-left(){
	{% if settings.enable_rtl %} 
