	@include rtl-margin-right(0px);
}
.featured-banner {
	position: relative;
	overflow: hidden;
	a {
		display: block;
	}
	img {
		display: block;
		width: 100%;
		max-width: 100%;
		transition: all .5s ease-in-out;
	}
}
.effect-one {
	&:hover {
		img {
			transform: scale(1.1);
		}
	}
}
.effect-two {
	img {
		transform: scale(1.1);
	}
	&:hover {
		img {
			transform: scale(1);
		}
	}
}
@keyframes effectShine {
	100% {
		left: 125%;
	}
}
.effect-three {
	&::before {
		content: '';
		pointer-events: none;
		position: absolute;
		top: 0;
		left: -75%;
		z-index: 2;
		display: block;
		width: 50%;
		height: 100%;
		background: linear-gradient(to right, rgba(255, 255, 255, 0) 0%, rgba(255, 255, 255, .3) 100%);
		transform: skewX(-25deg);
	}
	&:hover {
		&::before {
			animation: effectShine .75s;
		}
	}
}
.effect-four {
	&::before,
	&::after {
		content: '';
		pointer-events: none;
		position: absolute;
		display: block;
		transition: all .5s ease;
	}
	&::before {
		top: 5%;
		left: 0;
		right: 0;
		width: 0;
		height: 90%;
		margin: 0 auto;
		border-top: 1px solid $white;
		border-bottom: 1px solid $white;
	}
	&::after {
		left: 5%;
		top: 0;
		bottom: 0;
		width: 90%;
		height: 0;
		margin: auto;
		border-left: 1px solid $white;
		border-right: 1px solid $white;
	}
	&:hover {
		&::before {
			width: 90%;
			max-width: 90%;
		}
		&::after {
			height: 90%;
			max-height: 90%;
		}
	}
}

