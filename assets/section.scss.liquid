// SECTION BUTTON
.bwp-button{
	font-size: 11px;
	letter-spacing: 2px;
	display:inline-block !important;
	&.style_1{
		color: $white;
		position: relative;
		letter-spacing: 3px;
		padding-bottom: 3px;
		text-transform: uppercase;
		&:before, &:after {
			background: $white;
			bottom: 0;
			content: "";
			width: 100%;
			height: 1px;
			left: 0;
			position: absolute;
			transform-origin: right;
			transform: scaleX(1) translateZ(0);
			transition: transform .6s cubic-bezier(.165, .84, .44, 1) .3s;
		}
		&:after {
			transform: scaleX(0);
			transform-origin: left;
			transition: transform .6s cubic-bezier(.165, .84, .44, 1);
		}
		&:hover {
			&:before {
				transform: scaleX(0);
				transition: transform .6s cubic-bezier(.165, .84, .44, 1);
			}
			&:after {
				transform: scaleX(1);
				transition: transform .6s cubic-bezier(.165, .84, .44, 1) .3s;
			}
		}
	}
	&.style_2{
		line-height:50px;
		padding:0 25px;
		background:$white;
		color:$gray-dark;
		position: relative;
		overflow: hidden;
		font-style: italic;
		font-size: 22px;
		letter-spacing: 0;
		font-family: $font-family-second;
		span{
			width: 100%;
			height: 100%;
			display: inline-block;
			transition: all .3s ease;
			backface-visibility: hidden;
		}
		&:before{
			top: 0;
			position: absolute;
			content: "\e900";
			font-family: icomoon;
			height: 100%;
			width: 100%;
			left: -100%;
			color: transparent;
			line-height: 50px;
			font-size: 16px;
			transition: all .3s ease;
		}
		&:after{
			content: "";
			position: absolute;
			z-index: -1;
			transition: all .3s ease;
		}
		&:hover{
			span{
				color: $nocolor;
				transform: translateX(200%);
			}
			&:before{
				left: 0;
				color: $gray-dark;
			}
		}
	}
	&.style_3{
		line-height:35px;
		padding:0 25px;
		background: transparent;
		color: $gray-dark;
		position: relative;
		border: 1px solid $gray-dark;
		overflow: hidden;
		span{
			width: 100%;
			height: 100%;
			display: inline-block;
			transition: all .3s ease;
			backface-visibility: hidden;
		}
		&:before{
			top: 0;
			position: absolute;
			content: "\e900";
			font-family: icomoon;
			height: 100%;
			width: 100%;
			left: -100%;
			color: transparent;
			line-height: 45px;
			font-size: 16px;
			transition: all .3s ease;
		}
		&:after{
			content: "";
			position: absolute;
			z-index: -1;
			transition: all .3s ease;
		}
		&:hover{
			span{
				color: $nocolor;
				transform: translateX(200%);
			}
			&:before{
				left: 0;
				color: $gray-dark;
			}
		}
	}
}

