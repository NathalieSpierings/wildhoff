		.countdown-product {
			bottom: 10px;
			padding: 0 10px;
			.countdown {
				padding: 10px 5px;
			}
		}
	}
	.wpbingo-lookbook {
		overflow: hidden; 
	}
	 .wpbingo-lookbook__content {
		display: flex;
		flex-flow: row wrap;
		justify-content: center; 
	}
	.lookbook-card {
		position: relative;
		display:inline-block;
		.lookbook-image img{
			width:100%;
		}
	}
	.lookbook-card__item {
		position: absolute;
		z-index: 1;
	}
	.lookbook-card__item:hover, .lookbook-card__item:focus {
		z-index: 2; 
	}
	.lookbook-card__point {
		position: absolute;
		top: -15px;
		left: -15px;	
	}
	.lookbook-card__point{
		&:hover,&:focus{
			.product-card{
				opacity: 1;
				visibility: visible;
			}
		}
	}
	.lookbook-card__button {
		cursor: pointer; 
		&:hover {
			.lookbook-card__btn {
				background: $theme-color;
				color: $white;
				&:before,&:after{
					border-color:$theme-color;
				}
			}
		}
	}
	.lookbook-card__btn {
		position: relative;
		display: block;
		width: 30px;
		height: 30px;
		border: 0;
		padding: 0;
		background: $white;
		border-radius: 50%;
		transition: all .2s ease;
		font-size:0;
		line-height:30px;
		text-align:center;
		color:$gray-dark;
		&:before,&:after{
			pointer-events:none;
			position:absolute;
			width:100%;
			height:100%;
			left:0;
			top:0;
			content:"";
			border:1px solid $white;
			-webkit-animation-name: scale-lookbook;
			animation-name: scale-lookbook;
			-webkit-animation-duration: 3s;
			animation-duration: 3s;
			-webkit-animation-timing-function: ease-in-out;
			animation-timing-function: ease-in-out;
			-webkit-animation-iteration-count: infinite;
			animation-iteration-count: infinite;
			border-radius:50%;
		}
		&:after{
			animation-delay: 1.5s;
		}
		span{
			&:before{
				font-family: feather;
				content: "\e9b1";
				display:inline-block;
				font-size:19px;
			}
		}
	}
	.content-product-card {
		display: none;
		position: absolute;
		z-index: 2;
		width: 250px;
		opacity: 0;
		visibility: hidden;
		&:after {
			content: "";
			pointer-events: none;
			position: absolute; 
		}
		&.active{
			visibility: visible;
			opacity:1;
			display:block;
		}
		.product-card{
			padding: 10px 10px 20px;
			margin-bottom:0;
			text-align: center;
			background-color: white;
			transition: all .2s ease;
			box-shadow: -1px 5px 5px 0 rgba(0, 0, 0, 0.2);
			overflow:unset;
		}
	}
	.position-top-left .product-card{
		@include transform(translate(calc(-100% + 30px),calc(-100% - 15px)));
		position:relative;
		&:before{
			content:"";
			position:absolute;
			border:8px solid transparent;
			border-top-color:$white;
			border-width: 12px 8px;
			bottom: -23px;
			right: 7px;
		}
	}
	.position-top-right .product-card{
		@include transform(translateY(calc(-100% - 12px)));
		position:relative;
		&:before{
			content:"";
			position:absolute;
			border:8px solid transparent;
			border-top-color:$white;
			border-width: 12px 8px;
			bottom: -23px;
			left: 7px;
		}
	}
	.position-center-left .product-card{
		@include transform(translate(calc(-100% - 15px),-50%));
		position:relative;
		&:before{
			content:"";
			position:absolute;
			border:8px solid transparent;
			border-left-color:$white;
			border-width: 8px 12px;
			top: calc(50% + 6px);
			left:100%;
		}
	}
	.position-center-top .product-card{
		@include transform(translate(calc(-50% + 12px),calc(-100% - 15px)));
		position:relative;
		&:before{
			content:"";
			position:absolute;
			border:8px solid transparent;
			border-top-color:$white;
			border-width: 12px 8px;
			bottom: -23px;
			left: calc(50% - 6px);
		}
	}
	.position-center-right .product-card{
		@include transform(translate(45px,calc(-50% + 15px)));
		position:relative;
		&:before{
			content:"";
			position:absolute;
			border:8px solid transparent;
			border-right-color:$white;
			border-width: 8px 12px;
			top: calc(50% - 8px);
			left: -24px;
		}
	}
	.position-center-bottom .product-card{
		@include transform(translate(calc(-50% + 15px),45px));
		position:relative;
		&:before{
			content:"";
			position:absolute;
			border:8px solid transparent;
			border-bottom-color:$white;
			border-width: 12px 8px;
			top: -24px;
			left:calc(50% - 8px);
		}
	}
	.position-bottom-left .product-card{
		@include transform(translate(calc(-100% + 30px),45px));
		position:relative;
		&:before{
			content:"";
			position:absolute;
			border:8px solid transparent;
			border-bottom-color:$white;
			border-width: 12px 8px;
			top: -24px;
			right:8px;
		}
	}
	.position-bottom-right .product-card{
		@include transform(translateY(45px));
		position:relative;
		&:before{
			content:"";
			position:absolute;
			border:8px solid transparent;
			border-bottom-color:$white;
