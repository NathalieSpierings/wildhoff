// SLIDESHOW
.wpbingo-slideshow {
	margin: 0;
	&:not(.slick-initialized) .wpbingo-slideshow__item  {
		display: none;
		&:first-child{
			display:block;
		}
	}
	.slideshow-caption__btn{
		position:relative;
		display:inline-block;
		z-index:1;
		border:0;
		text-transform: revert;
		&:before{
			position:absolute;
			right:0;
			content:"";
			height:100%;
			width:0;
			top:0;
			z-index:-1;
			transition:all 0.3s ease;
		}
		&:hover{
			&:before{
				width:100%;
				left:0;
			}
		}
	}
	.wpbingo-slideshow__item-image.mobile{
		@media (min-width:767px){
			display:none;
		}
	}
	.wpbingo-slideshow__item{
		position:relative;
	}
	.slideshow-caption__inner{
		display:flex;
		flex-wrap:wrap;
	}
	.slideshow-caption{
		position: absolute;
		top: 0;
		width: 100%;
		left: 0;
		height: 100%;
		display:flex;
		opacity:0;
		&.disable_absolute{
			@media (max-width:767px){
				position:static;
			}
		}
	}
	.slick-arrow {
		width: 45px;
		height: 45px;
		margin-top: -22px;
		line-height:1;
		opacity:1;
		transition: unset;
		visibility:visible;
		top:calc(50% - 22px);
		@media screen and (max-width: 991px) {
			display:none !important;
		}
	}
	.slick-prev,.slick-next {
		&::before {
			font-size: $font-size-base + 1;
			transition: all .3s ease;
		}
	}
	.slick-prev {
		left: 50px;
		&:before {
			content: "\e910";
			font-family: icomoon;
		}
	}
	.slick-next {
		right: 50px;
		&:before {
			content: "\e904";
			font-family: icomoon;
		}
	}
	.slick-dots {
		bottom: 50px;
		@media screen and (max-width: 1050px) {
			bottom: 30px;
		}
		@media screen and (max-width: 767px) {
			bottom: 10px;
		}
		li {
			margin: 0 5px;
			width:22px;
			height:22px;
			border-radius:50%;
			border-width:1px;
			display: inline-flex;
			align-items: center;
			padding: 0;
			button{
				width:7px;
				height:7px;
				margin: auto;
			}
			&.slick-active{
				border-style:solid;
			}
		}
	}
	.slick-slide {
		position: relative;
	}
	.slick-list{
		margin:0;
		.wpbingo-slideshow__item{
			padding:0;
		}
	}
	.slick-active .slideshow-caption{
		opacity:1;
	}
	.image-size--cover .wpbingo-slideshow__item-image{
		object-fit: cover;
	}
	.image-size--contain .wpbingo-slideshow__item-image{
		object-fit: contain;
	}
	.image-size--fill .wpbingo-slideshow__item-image{
		object-fit: fill;
	}
	.image-pos--left_top .wpbingo-slideshow__item-image{
		object-position: left top;
	}
	.image-pos--left_center .wpbingo-slideshow__item-image{
		object-position: left center;
	}
	.image-pos--left_bottom .wpbingo-slideshow__item-image{
		object-position: left bottom;
	}
	.image-pos--center_top .wpbingo-slideshow__item-image{
		object-position: center top;
	}
	.image-pos--center_center .wpbingo-slideshow__item-image{
		object-position: center center;
	}
	.image-pos--center_bottom .wpbingo-slideshow__item-image{
		object-position: center bottom;
	}
	.image-pos--right_top .wpbingo-slideshow__item-image{
		object-position: right top;
	}
	.image-pos--right_center .wpbingo-slideshow__item-image{
		object-position: right center;
	}
	.image-pos--right_bottom .wpbingo-slideshow__item-image{
		object-position: right bottom;
	}
	.slideshow-text--left{
		text-align: left;
	}
	.slideshow-text--center{
		text-align: center;
	}
	.slideshow-text--right{
