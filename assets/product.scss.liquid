// PRODUCT MODEL
.product-single__media-group {
  &.slick-slider {
    margin: 0;
    overflow: hidden;
  }

  .slick-slide,
  .slick-track {
    &:focus {
      outline: 0;
    }
  }

  &.slick-initialized {
    ~ .main-media__slick-controls {
      display: block;
    }
  }
}

.main-media__slick-controls {
  display: none;
}

.product-single__media--stacked {
	.product-single__media-item {
		margin-bottom: 15px;
	}
}

.product-media {
  &:focus {
    outline: 0;
  }
}

.product-single__view-in-space {
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  z-index: 2;
  width: 100%;
  min-height: 44px;
  margin: 0;
  padding: 10px;
  font-size: $font-size;
  color: $gray-dark;
  background-color: rgba($gray-dark, .08);
  border: none;
  transition: all .3s ease;

  svg {
    display: block;
    height: 16px;
    width: 14px;
    @include rtl-margin-right(8px);
  }

  &[data-shopify-xr-hidden] {
    @media (min-width: 768px) {
      display: none;
    }
  }

  &:hover,
  &:focus {
    outline: 0;
  }
}

.product-single__view-in-space--disabled {
  display: none;
}
