// photoswipe
.pswp{
	position:fixed;
	width:100%;
	top:0;
	left:0;
	z-index:10000;
	display:none;
	.pswp__img--placeholder--blank{
		background:$white;
		&:before{
			position: absolute;
			display: inline-block;
			content: "";
			border: 4px solid rgba($gray-dark,.25);
			border-top-color: $gray-dark;
			width: 50px;
			height: 50px;
			background: none;
			-webkit-animation: 2s linear 0s normal none infinite running spin-load;
			animation: 2s linear 0s normal none infinite running spin-load;
			text-indent: 0;
			top: calc(50% - 25px);
			left: calc(50% - 25px);
			border-radius: 50%;
			-webkit-border-radius: 50%;
			-moz-border-radius: 50%;
			-ms-border-radius: 50%;
			-o-border-radius: 50%;
		}
	}
	.pswp__bg{
		background:$white;
	}
	&.pswp--open{
		display:block;
		.pswp__ui{
			position: fixed;
			bottom: 50px;
			display: flex;
			width: 100%;
			align-items:center;
			justify-content:center;
		}
		.pswp__top-bar{
			position:relative;
			z-index:9;
			.pswp__button{
				width: 55px;
				height: 55px;
				border:0;
				padding:0;
				cursor:pointer;
				box-shadow: 0 2px 10px rgba($gray-dark, 0.15) !important;
				background:$white !important;
				border-radius:50%;
				margin:0 5px;
				color:$gray-dark;
				&.pswp__button--close{
					&:before{
						content:"\ea02";
						font-family:feather;
						display:inline-block !important;
						font-size:23px;
						line-height:55px;
					}
				}
				&.pswp__button--share{
					display:none;
				}
				&.pswp__button--fs{
					&:before{
						content:"\e98d";
						font-family:feather;
						display:inline-block !important;
						font-size:23px;
						line-height:55px;
					}
				}
				&:hover{
					background:$theme-color !important;
					color:$white;
				}
			}
		}
		.pswp__button--arrow--left{
			font-family: feather;
			border:0;
			padding:0;
			font-weight:500;
			font-size:20px;
			color:$gray-dark;
			cursor:pointer;
			width:45px;
			height:45px;
			border-radius:50%;
			margin:0 5px;
			background:$white !important;
			box-shadow: 0 2px 10px rgba($gray-dark, 0.15) !important;
			&:before{
				content:"\e92f";
			}
			&:hover{
				background:$theme-color !important;
				color:$white;
			}
		}
		.pswp__button--arrow--right{
			font-family: feather;
			border:0;
			padding:0;
			font-weight:500;
			font-size:20px;
			color:$gray-dark;
			cursor:pointer;
			width:45px;
			height:45px;
			border-radius:50%;
			margin:0 5px;
			background:$white !important;
			box-shadow: 0 2px 10px rgba($gray-dark, 0.15) !important;
			&:before{
				content: "\e930";
			}
			&:hover{
				background:$theme-color !important;
				color:$white;
			}
		}
	}
}
